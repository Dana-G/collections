<!DOCTYPE html>
<html>
<head>
  <title>Collections</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="container">
    <div id="user-header">
        <%= link_to "Sign Up", signup_path %>
    </div>

  <% flash.each do |name, msg| %>
  <%= content_tag :div, msg, id: "flash_#{name}" %>
 <% end %>
<%= yield %>

</body>
</html>
